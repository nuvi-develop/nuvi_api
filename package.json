{
  "name": "nuvi-api",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm run prod",
    "server": "babel-node ./src/bin/www",
    "server:prod": "node ./dist/bin/www",
    "transfile": "babel ./src --out-dir dist",
    "clean": "rimraf dist",
    "build": "npm-run-all clean transfile",
    "dev": "NODE_ENV=development npm-run-all server",
    "dev:light": "NODE_ENV=development_light npm-run-all server",
    "prod": "NODE_ENV=production npm-run-all build server:prod",
    "whatch:dev": "nodemon",
    "migrate": "cd src/sequelize && npx sequelize-cli db:migrate",
    "migrate:production": "NODE_ENV=production npm run migrate",
    "seed:department": "cd src/sequelize && npx sequelize-cli db:seed --seed 20200325125309-demo-department && cd ../..",
    "seed:department:production": "NODE_ENV=production npm run seed:department",
    "seed:unit": "cd src/sequelize && npx sequelize-cli db:seed --seed 20200601062432-demo-ingredientUnits && cd ../..",
    "seed:unit:production": "NODE_ENV=production npm run seed:unit",
    "seed:inventoryCategory": "cd src/sequelize && npx sequelize-cli db:seed --seed 20200601062432-demo-inventoryCategory && cd ../..",
    "seed:inventoryCategory:production": "NODE_ENV=production npm run seed:inventoryCategory",
    "seed:inventoryIngredient": "cd src/sequelize && npx sequelize-cli db:seed --seed 20200601062446-demo-inventoryIngredient && cd ../..",
    "seed:inventoryLog": "cd src/sequelize && npx sequelize-cli db:seed --seed 20200601062450-demo-inventoryLog && cd ../..",
    "seed:all": "NODE_ENV=development npm run seed:department && npm run seed:unit && npm run seed:inventoryCategory &&npm run seed:inventoryIngredient &&npm run seed:inventoryLog",
    "sync:database:development": "NODE_ENV=development babel-node ./src/sequelize/sync",
    "sync:database:production": "NODE_ENV=production babel-node ./src/sequelize/sync"
  },
  "dependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "date-fns": "^2.14.0",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "google-auth-library": "^6.0.0",
    "immer": "^6.0.9",
    "jsonwebtoken": "^8.5.1",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "nodemailer": "^6.4.6",
    "nodemailer-smtp-pool": "^2.8.3",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "sequelize": "^5.21.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "sequelize-cli": "^5.5.1"
  }
}
